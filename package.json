{
  "name": "starter-nest",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "description": "_description_",
  "author": "LaiBaoYuan <1727283040@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/LaiBaoYuan/starter-nest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaiBaoYuan/starter-nest.git"
  },
  "bugs": "https://github.com/LaiBaoYuan/starter-nest/issues",
  "scripts": {
    "build": "nest build",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.5.0",
    "@nestjs/cli": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.16.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "typescript": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm check"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
